{% extends 'base.html.twig' %}

{% block title %}Liste des contacts{% endblock %}

{% block body %}
    <h1>Liste des contacts</h1>
    <ul>
    {% for contact in contacts %}
        <li><a href="{{ path('app_contact_show', {id : contact.id}) }}"> {{ contact.lastname }}, {{ contact.firstname }}</a></li>
    {% endfor %}
    </ul>
{% endblock %}
